{
  "name": "canvas-project-manager",
  "version": "1.8.16",
  "description": "Visual project management on Obsidian Canvas - manage milestones, stories, tasks, decisions, documents, and features with dependencies and Notion sync",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "keywords": [
    "obsidian",
    "plugin",
    "canvas",
    "notion",
    "accomplishments",
    "project-management",
    "workflow"
  ],
  "author": "Ostan Labs",
  "license": "MIT",
  "devDependencies": {
    "@eslint/json": "^0.14.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@typescript-eslint/utils": "^8.50.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.20.1",
    "eslint": "^9.39.2",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.50.0"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15"
  }
}
